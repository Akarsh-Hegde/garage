{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "declaration": true,
      "declarationDir": "dist",
      "outDir": "dist",
      "moduleResolution": "Node"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  